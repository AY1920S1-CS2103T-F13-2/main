<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<fx:root resizable="false" title="Info" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene stylesheets="@DarkTheme.css">
         <VBox spacing="10.0" styleClass="info-window">
            <children>
               <Label fx:id="commandWord" contentDisplay="CENTER" styleClass="info-header" text="Label" textAlignment="CENTER" textFill="WHITE" VBox.vgrow="ALWAYS">
                  <font>
                     <Font name="Franklin Gothic Heavy" size="52.0" />
                  </font>
               </Label>
                    <Label fx:id="infoMessage" maxWidth="600.0" prefWidth="600.0" styleClass="info-small-label" text="Label" textFill="WHITE" wrapText="true">
                  <font>
                     <Font name="Franklin Gothic Demi Cond" size="20.0" />
                  </font>
                    </Label>
               <Label styleClass="info-big-label" text="USAGE :" textFill="WHITE">
                  <font>
                     <Font name="Franklin Gothic Heavy" size="25.0" />
                  </font>
               </Label>
               <Label fx:id="usageExample" maxWidth="600.0" prefWidth="600.0" styleClass="info-small-label" text="Label" textFill="WHITE" wrapText="true">
                  <font>
                     <Font name="Courier New" size="20.0" />
                  </font>
               </Label>
               <Label styleClass="info-big-label" text="EXPECTED OUTPUT :" textFill="WHITE">
                  <font>
                     <Font name="Franklin Gothic Heavy" size="25.0" />
                  </font>
               </Label>
               <Label fx:id="output" maxWidth="600.0" prefWidth="600.0" styleClass="info-small-label" text="Label" textFill="WHITE" wrapText="true">
                  <font>
                     <Font name="Courier New" size="20.0" />
                  </font>
               </Label>
               <Button mnemonicParsing="false" onAction="#closeWindow" styleClass="help-button" text="Close">
                  <font>
                     <Font name="Franklin Gothic Demi Cond" size="36.0" />
                  </font>
                  <VBox.margin>
                     <Insets top="20.0" />
                  </VBox.margin>
               </Button>
            </children>
            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
            </padding>
         </VBox>
        </Scene>
    </scene>
</fx:root>
