<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.stage.Stage?>

<fx:root height="900.0" maximized="true" minHeight="900.0" minWidth="1400.0" onCloseRequest="#handleExit" title="\$aveNUS" type="javafx.stage.Stage" width="1400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/savenus_logo.png" />
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@DarkTheme.css" />
            </stylesheets>
            <HBox>
                <children>
                    <AnchorPane styleClass="menu-pane-with-border" HBox.hgrow="NEVER">
                        <children>
                            <Button layoutX="2.0" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" onAction="#handleHelp" prefHeight="50.0" prefWidth="50.0" styleClass="menu-button">
                                <graphic>
                                    <ImageView fitHeight="30.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                                        <image>
                                            <Image url="@../images/Help.png" />
                                        </image>
                                    </ImageView>
                                </graphic>
                        <padding>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                            </Button>
                            <Button layoutX="2.0" layoutY="50.0" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" onAction="#handleExit" prefHeight="50.0" prefWidth="50.0" styleClass="menu-button">
                                <graphic>
                                    <ImageView fitHeight="30.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                                        <image>
                                            <Image url="@../images/Quit.png" />
                                        </image>
                                    </ImageView>
                                </graphic>
                        <padding>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                            </Button>
                     <MenuButton layoutX="2.0" layoutY="100.0" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" popupSide="RIGHT" prefHeight="50.0" prefWidth="50.0">
                       <items>
                         <MenuItem mnemonicParsing="false" onAction="#changeThemeToDark" text="Dark" />
                         <MenuItem mnemonicParsing="false" onAction="#changeThemeToLight" text="Light" />
                       </items>
                        <graphic>
                           <ImageView fitHeight="30.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/ThemeLight.png" />
                              </image>
                           </ImageView>
                        </graphic>
                     </MenuButton>
                        </children>
                    </AnchorPane>
                    <VBox maxWidth="1.7976931348623157E308" prefWidth="1830.0" HBox.hgrow="ALWAYS">
                        <children>
                            <StackPane fx:id="commandBoxPlaceholder" styleClass="pane-with-border" VBox.vgrow="NEVER">
                                <padding>
                                    <Insets bottom="5" left="10" right="10" top="5" />
                                </padding>
                            </StackPane>
                            <StackPane fx:id="resultDisplayPlaceholder" maxHeight="150.0" minHeight="150.0" styleClass="pane-with-border" VBox.vgrow="NEVER">
                                <padding>
                                    <Insets bottom="5" left="10" right="10" top="5" />
                                </padding>
                            </StackPane>
                            <HBox VBox.vgrow="ALWAYS">
                                <children>
                                    <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="500.0" HBox.hgrow="ALWAYS">
                                        <VBox fx:id="foodList" prefHeight="655.0" styleClass="pane-with-border" VBox.vgrow="ALWAYS">
                                            <StackPane fx:id="foodListPanelPlaceholder" VBox.vgrow="ALWAYS">
                                                <VBox.margin>
                                                    <Insets />
                                                </VBox.margin>
                                            </StackPane>
                                 <padding>
                                    <Insets bottom="10.0" />
                                 </padding>
                                        </VBox>
                                    </VBox>
                           <VBox styleClass="vbox-tab-pane-with-border">
                              <children>
                                 <HBox alignment="CENTER" spacing="20.0">
                                    <children>
                                       <ImageView fitHeight="167.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
                                          <image>
                                             <Image url="@../images/Wallet.png" />
                                          </image>
                                       </ImageView>
                                       <VBox alignment="CENTER_LEFT">
                                          <children>
                                                      <Text fx:id="savingsAccountPlaceholder" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="account-balance" text="savings" wrappingWidth="400.0">
                                                          <font>
                                                              <Font name="Monospaced Regular" size="15.0" />
                                                          </font>
                                                      </Text>
                                                        <Text fx:id="remainingBudgetPlaceholder" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="account-balance" text="money" wrappingWidth="400.0">
                                                            <font>
                                                                <Font name="Monospaced Regular" size="15.0" />
                                                            </font>
                                                        </Text>
                                                        <Text fx:id="daysToExpirePlaceholder" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="account-days-left" text="days" wrappingWidth="400.0">
                                                            <font>
                                                                <Font name="Franklin Gothic Demi Cond" size="27.0" />
                                                            </font>
                                                        </Text>
                                          </children>
                                       </VBox>
                                    </children>
                                    <padding>
                                       <Insets bottom="20.0" top="20.0" />
                                    </padding>
                                 </HBox>
                                 <TabPane styleClass="tab-pane-with-border" tabClosingPolicy="UNAVAILABLE">
                                   <tabs>
                                     <Tab text="Purchases">
                                       <content>
                                             <VBox maxHeight="1.7976931348623157E308" prefHeight="655.0" prefWidth="670.0" spacing="10.0" styleClass="vbox-inner-tab-pane-with-border">
                                                <children>
                                                               <Text fill="white" styleClass="purchase-history-text" text="Purchase History" textAlignment="CENTER" wrappingWidth="630.0" VBox.vgrow="NEVER">
                                                                   <font>
                                                                       <Font name="Franklin Gothic Demi Cond" size="40.0" />
                                                                   </font>
                                                               </Text>
                                                   <VBox alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" styleClass="pane-with-border" VBox.vgrow="ALWAYS">
                                                      <children>
                                                         <StackPane fx:id="purchaseListPanelPlaceholder" maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                                                            <VBox.margin>
                                                               <Insets />
                                                            </VBox.margin>
                                                         </StackPane>
                                                      </children>
                                                   </VBox>
                                                </children>
                                                <padding>
                                                   <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                                                </padding>
                                             </VBox>
                                       </content>
                                     </Tab>
                                     <Tab closable="false" text="Savings">
                                          <content>
                                                        <VBox maxHeight="1.7976931348623157E308" spacing="10.0" styleClass="vbox-inner-tab-pane-with-border">
                                                <Text fill="white" styleClass="purchase-history-text" text="Savings History" textAlignment="CENTER" wrappingWidth="630.0">
                                                   <font>
                                                      <Font name="Franklin Gothic Demi Cond" size="40.0" />
                                                   </font>
                                                </Text>
                                                            <VBox maxHeight="1.7976931348623157E308" styleClass="pane-with-border" VBox.vgrow="ALWAYS">
                                                                <StackPane fx:id="savingsHistoryPanelPlaceholder" maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                                                                    <VBox.margin>
                                                                        <Insets />
                                                                    </VBox.margin>
                                                                </StackPane>
                                                            </VBox>
                                                <padding>
                                                   <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                                                </padding>
                                                        </VBox>
                                          </content>
                                     </Tab>
                                   </tabs>
                                 </TabPane>
                              </children>
                           </VBox>
                                </children>
                            </HBox>
                               <StackPane fx:id="statusbarPlaceholder" />
                        </children>
                    </VBox>
                </children>
            </HBox>
        </Scene>
    </scene>
</fx:root>
